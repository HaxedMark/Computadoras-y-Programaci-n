#include<stdio.h>

using namespace std;

int main (int argc, char *argv[]) {
	int var1=5;
	int var2=2;
	int resultado=0;
	
	//suma
	resultado=var1 +var2;
	printf("%d+%d=%d\n",var1,var2,resultado);
	
	//resta
	resultado=var1 - var2;
	printf("%d-%d=%d\n",var1,var2,resultado);
	
	//multiplicaciÃ³n
	resultado=var1 * var2;
	printf("%d*%d=%d\n",var1,var2,resultado);
	
	//division
	resultado=var1 / var2;
