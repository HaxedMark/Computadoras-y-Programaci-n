#include<stdio.h>
#include<iostream>
using namespace std;

void fnrec(int a,int b,int x,int y){
	
	for(a=1;a<x;a=a+1){
	for(b=1;b<y;b=b+1){
		printf("$");
	}	
	cout<<"\n";
    }
	}
	
	int main(){
	int x=0,y=0,a=0,b=0;
	cout <<"inserte ancho \n";
	scanf("%d",&y);
	
	cout <<"Digite el Alto x\n";
	scanf("%d",&x);
	fnrec(a,b,x+1,y+1);
}
